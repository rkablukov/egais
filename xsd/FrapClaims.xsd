            <xs:element name="TechReglStandard"  minOccurs="1" maxOccurs="unbounded">
              <xs:annotation>
                <xs:documentation>ТехРеглСтандарт.</xs:documentation>
              </xs:annotation>
              <xs:complexType>
                <xs:choice>
                  <xs:element name="TechRegl" nillable="false" minOccurs="1" maxOccurs="1">
                    <xs:annotation>
